{
  "name": "jp-visouldocs",
  "version": "7.1.1",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build",
    "start": "ts-node --esm server.ts",
    "clean": "scripts/clean.sh && pnpm install",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix --no-warn-ignored",
    "type-check": "tsc --noEmit",
    "check": "pnpm type-check && pnpm lint && pnpm alias:check",
    "validate": "pnpm type-check && pnpm lint && pnpm alias:validate",
    "alias:check": "node scripts/alias.ts --check",
    "alias:fix": "node scripts/alias.ts --fix",
    "alias:validate": "node scripts/alias.ts --validate",
    "postcss:watch": "tailwindcss -i ./src/index.css -o ./dist/output.css --watch",
    "prepare": "husky install",
    "test": "vitest run",
    "test:ui": "vitest --ui --coverage"
  },
